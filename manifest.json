{
    "manifest_version": 2,
    "name":    "Test Extension",
    "version": "0.0",
    "offline_enabled": true,

    "permissions": [
      "storage"
    ],
  
    "background": {
      "persistent": false,
      "scripts": ["background.js"]
    },
  
    "content_scripts": [{
      "matches":    ["http://www.csun.edu/exlinfo/dl/support/techsupport_staff.php"],
      "css":        ["styles.css"],
      "js":         ["jquery-3.3.1.min.js", "typeahead.bundle.min.js", "content.js"],
      "run_at":     "document_idle",
      "all_frames": false
    }],
  
    "page_action": {
      "default_title": "Test Extension",
      "default_popup": "popup.html"
    }
  
 
  }